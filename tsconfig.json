{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@providers/*": ["src/providers/*"],
      "@components/*": ["src/components/*"],
      "@sections/*": ["src/components/_sections/*"],
      "@public/*": ["public/*"],
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/Link.js", "src/Link.js"  ],
  "exclude": [
    "node_modules"
  ]
}
